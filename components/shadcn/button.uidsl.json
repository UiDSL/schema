{
  "type": "button",
  "props": {
    "variant": "default",
    "size": "md",
    "text": "Button"
  },
  "styleConfig": {
    "variant": {
      "default": "bg-primary text-white",
      "destructive": "bg-destructive text-white",
      "outline": "border border-input text-foreground"
