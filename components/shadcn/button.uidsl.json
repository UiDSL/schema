    "size": {
      "sm": "h-8 px-3 text-sm",
      "md": "h-10 px-4 text-base",
      "lg": "h-12 px-6 text-lg"
    }
  },
  "description": "ShadCN-style button with variant and size props for Tailwind styling",
  "$id": "Button",
  "import": "components/shadcn/button.uidsl.json"
}